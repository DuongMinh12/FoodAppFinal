<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppFood.Views.SettingPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Danh mục quản lý" x:Name="btCatagory"
                    Clicked="btCatagory_Clicked"/>
            <Button Text="Danh mục sản phẩm" x:Name="btProduct"
                    Clicked="btProduct_Clicked"/>
            <Button Text="Danh mục giỏ hàng" x:Name="btcart"
                    Clicked="btcart_Clicked"/>
            <Button Text="home" x:Name="bthome"
                    Clicked="bthome_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>